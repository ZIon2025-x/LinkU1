{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "link2ur.com"
        }
      ],
      "destination": "https://www.link2ur.com/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "x-forwarded-proto",
          "value": "http"
        }
      ],
      "destination": "https://www.link2ur.com/$1",
      "permanent": true
    },
    {
      "source": "/tasks",
      "destination": "/en/tasks",
      "permanent": true
    },
    {
      "source": "/about",
      "destination": "/en/about",
      "permanent": true
    },
    {
      "source": "/partners",
      "destination": "/en/partners",
      "permanent": true
    },
    {
      "source": "/faq",
      "destination": "/en/faq",
      "permanent": true
    },
    {
      "source": "/join-us",
      "destination": "/en/join-us",
      "permanent": true
    },
    {
      "source": "/terms",
      "destination": "/en/terms",
      "permanent": true
    },
    {
      "source": "/privacy",
      "destination": "/en/privacy",
      "permanent": true
    },
    {
      "source": "/merchant-cooperation",
      "destination": "/en/merchant-cooperation",
      "permanent": true
    },
    {
      "source": "/contact",
      "destination": "/en/faq",
      "permanent": true
    },
    {
      "source": "/",
      "destination": "/en/",
      "permanent": true
    },
    {
      "source": "/en",
      "destination": "/en/",
      "permanent": true
    },
    {
      "source": "/zh",
      "destination": "/zh/",
      "permanent": true
    },
    {
      "source": "/favicon.ico",
      "destination": "/static/favicon.ico",
      "permanent": true
    },
    {
      "source": "/favicon.png",
      "destination": "/static/favicon.ico",
      "permanent": true
    },
    {
      "source": "/apple-touch-icon.png",
      "destination": "/static/favicon.ico",
      "permanent": true
    },
    {
      "source": "/apple-touch-icon-precomposed.png",
      "destination": "/static/favicon.ico",
      "permanent": true
    },
    {
      "source": "/favicon-16x16.png",
      "destination": "/static/favicon.ico",
      "permanent": true
    },
    {
      "source": "/favicon-32x32.png",
      "destination": "/static/favicon.ico",
      "permanent": true
    },
    {
      "source": "/favicon-48x48.png",
      "destination": "/static/favicon.ico",
      "permanent": true
    },
    {
      "source": "/favicon-96x96.png",
      "destination": "/static/favicon.ico",
      "permanent": true
    },
    {
      "source": "/favicon-128x128.png",
      "destination": "/static/favicon.ico",
      "permanent": true
    },
    {
      "source": "/favicon-192x192.png",
      "destination": "/static/favicon.ico",
      "permanent": true
    },
    {
      "source": "/favicon-256x256.png",
      "destination": "/static/favicon.ico",
      "permanent": true
    },
    {
      "source": "/favicon-512x512.png",
      "destination": "/static/favicon.ico",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(), microphone=(), camera=()"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://www.google.com https://www.gstatic.com https://js.stripe.com https://connect-js.stripe.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com; img-src 'self' data: https: blob:; connect-src 'self' https://api.link2ur.com wss://api.link2ur.com https://www.google.com; frame-src https://js.stripe.com https://connect-js.stripe.com https://www.google.com;"
        }
      ]
    },
    {
      "source": "/static/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/static/favicon.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/static/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/static/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/static/css/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://api.link2ur.com/api/$1"
    },
    {
      "source": "/uploads/(.*)",
      "destination": "https://api.link2ur.com/uploads/$1"
    },
    {
      "source": "/zh/tasks/:id",
      "destination": "https://api.link2ur.com/zh/tasks/:id"
    },
    {
      "source": "/en/tasks/:id",
      "destination": "https://api.link2ur.com/en/tasks/:id"
    },
    {
      "source": "/tasks/:id",
      "destination": "https://api.link2ur.com/tasks/:id"
    },
    {
      "source": "/zh/activities/:id",
      "destination": "https://api.link2ur.com/zh/activities/:id"
    },
    {
      "source": "/en/activities/:id",
      "destination": "https://api.link2ur.com/en/activities/:id"
    },
    {
      "source": "/activities/:id",
      "destination": "https://api.link2ur.com/activities/:id"
    },
    {
      "source": "/zh/forum/post/:id",
      "destination": "https://api.link2ur.com/zh/forum/post/:id"
    },
    {
      "source": "/en/forum/post/:id",
      "destination": "https://api.link2ur.com/en/forum/post/:id"
    },
    {
      "source": "/forum/post/:id",
      "destination": "https://api.link2ur.com/forum/post/:id"
    },
    {
      "source": "/zh/leaderboard/custom/:id",
      "destination": "https://api.link2ur.com/zh/leaderboard/custom/:id"
    },
    {
      "source": "/en/leaderboard/custom/:id",
      "destination": "https://api.link2ur.com/en/leaderboard/custom/:id"
    },
    {
      "source": "/leaderboard/custom/:id",
      "destination": "https://api.link2ur.com/leaderboard/custom/:id"
    },
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/sitemap.xml",
      "destination": "https://api.link2ur.com/sitemap.xml"
    },
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/.well-known/ai.txt",
      "destination": "/.well-known/ai.txt"
    },
    {
      "source": "/baidu_verify_codeva-N1G3JdDSeL.html",
      "destination": "/baidu_verify_codeva-N1G3JdDSeL.html"
    },
    {
      "source": "/BingSiteAuth.xml",
      "destination": "/BingSiteAuth.xml"
    },
    {
      "source": "/a8d23d88bd2fc2123f47c1d280e364d2.txt",
      "destination": "/a8d23d88bd2fc2123f47c1d280e364d2.txt"
    },
    {
      "source": "/logo192.png",
      "destination": "/logo192.png"
    },
    {
      "source": "/logo512.png",
      "destination": "/logo512.png"
    },
    {
      "source": "/manifest.json",
      "destination": "/manifest.json"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://api.link2ur.com",
    "REACT_APP_WS_URL": "wss://api.link2ur.com"
  }
}
